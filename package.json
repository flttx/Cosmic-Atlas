{
  "name": "cosmic-atlas",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "node scripts/fetch-mpc-data.mjs",
    "assets:fetch": "powershell -ExecutionPolicy Bypass -File scripts/fetch-assets.ps1",
    "assets:fetch-nasa": "powershell -ExecutionPolicy Bypass -File scripts/fetch-nasa-assets.ps1",
    "assets:fetch-solar": "powershell -ExecutionPolicy Bypass -File scripts/fetch-solar-system-textures.ps1",
    "assets:fetch-gallery": "node scripts/fetch-nasa-gallery.mjs",
    "assets:fetch-models": "powershell -ExecutionPolicy Bypass -File scripts/fetch-nasa-models.ps1",
    "assets:build-nasa": "node scripts/build-nasa-maps.mjs",
    "assets:convert-nasa": "powershell -ExecutionPolicy Bypass -File scripts/convert-nasa-textures.ps1",
    "assets:convert-solar": "powershell -ExecutionPolicy Bypass -File scripts/convert-solar-system-textures.ps1",
    "assets:convert-gallery": "powershell -ExecutionPolicy Bypass -File scripts/convert-space-textures.ps1",
    "assets:install-ktx": "powershell -ExecutionPolicy Bypass -File scripts/install-ktx.ps1",
    "assets:setup": "node scripts/copy-basis.mjs",
    "assets:copy-draco": "powershell -ExecutionPolicy Bypass -File scripts/copy-draco.ps1",
    "assets:inspect": "gltf-transform inspect",
    "assets:gltf:optimize": "gltf-transform optimize",
    "assets:gltf:meshopt": "gltf-transform meshopt",
    "assets:gltf:etc1s": "gltf-transform etc1s",
    "assets:gltf:uastc": "gltf-transform uastc",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "test": "vitest run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "@headlessui/react": "^2.2.9",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@vercel/postgres": "^0.10.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sass": "^1.97.1",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@gltf-transform/cli": "^4.2.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jpeg-js": "^0.4.4",
    "pngjs": "^7.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
